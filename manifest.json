{
  "manifest_version": 2,

  "name": "Feminazi a flor",
  "description": "Reemplaza 'feminazi' con 'flor'",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Feminazi a flor"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
